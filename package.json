{
  "name": "micro-tania",
  "description": "My boss needed to automate her most common sayings",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "fun",
    "tania"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/micro-tania.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/micro-tania/issues"
  },
  "homepage": "https://github.com/zrrrzzt/micro-tania",
  "now": {
    "alias": [
      "tania.allthethings.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "axios": "0.18.0",
    "coveralls": "3.0.0",
    "micro-dev": "2.2.1",
    "nsp": "3.2.1",
    "nyc": "11.6.0",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "markdown-it": "8.4.1",
    "markov-generator": "1.2.2",
    "micro": "9.1.4"
  }
}
